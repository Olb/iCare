<?xml version='1.0'?>
<Section name="FollowUpResults">
    <Data>
        <Element type="BooleanFormElement" name="PreOp"/>
        <Element type="BooleanFormElement" name="Or"/>
        <Element type="BooleanFormElement" name="Pacu"/>
        <Element type="BooleanFormElement" name="PostOpLess24"/>
        <Element type="BooleanFormElement" name="PostOp24to48"/>
        <Element type="BooleanFormElement" name="PostOpGreater48"/>
        <Element type="BooleanFormElement" name="NoChange"/>
        <Element type="BooleanFormElement" name="AdditionalPtCare"/>
        <Element type="BooleanFormElement" name="NotApplicable"/>
        <Element type="BooleanFormElement" name="NoInjury"/>
