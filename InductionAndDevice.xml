<?xml version='1.0'?>
<Section name="InductionAndDevice">
    <Data>
        <Element type="BooleanFormElement" name="Iv"/>
        <Element type="BooleanFormElement" name="Im"/>
        <Element type="BooleanFormElement" name="Inhalation"/>
        <Element type="BooleanFormElement" name="PreO2"/>
        <Element type="BooleanFormElement" name="RapidSequence"/>
        <Element type="BooleanFormElement" name="OralAirway"/>
        <Element type="BooleanFormElement" name="LMA"/>
        <Element type="BooleanFormElement" name="NasalAirway"/>
        <Element type="BooleanFormElement" name="Mask"/>
        <Element type="TextElement" name="Size"/>
    </Data>
    <Validation>
        <RadioButtons>
            <Element name="Iv"/>
            <Element name="Im"/>
            <Element name="Inhalation"/>
        </RadioButtons>
        <RadioButtons>
            <Element name="OralAirway"/>
            <Element name="NasalAirway"/>
            <Element name="Mask"/>
        </RadioButtons>
    </Validation>

    <Graphics>
        <Group orientation="horizontal">
            <Group label="Induction" orientation="vertical">
                <Element name="Iv" label="IV" />
                <Element name="Im" label="IM" />
                <Element name="Inhalation" label="Inhalation" />
                <Element name="PreO2" label="Pre-O2" />
                <Element name="RapidSequence" label="Rapid sequence" />
            </Group>
            <Group label="Device" orientation="vertical">
                <Element name="OralAirway" label="Oral Airway" />
                <Element name="NasalAirway" label="Nasal Airway" />
                <Element name="Mask" label="Mask" />
                <Element name="LMA" label="LMA" />
                <Element name="Size" label="Size:" />
            </Group>
        </Group>
    </Graphics>
</Section>