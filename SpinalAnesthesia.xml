                    </Group>
                    <Group orientation="horizontal" >
                        <Element name="MedOther" />
                        <Element name="MedOtherReason"/>
                    </Group>
                    <Group orientation="horizontal" >
                        <Element name="Fentanyl"  label="Fentanyl"/>
                        <Element label="mcg"/>
                    </Group>
                    <Group orientation="horizontal" >
                        <Element name="Duramorph"  label="Duramorph"/>
