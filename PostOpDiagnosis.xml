<?xml version='1.0'?>
<Section name="PostOpDiagnosis">
    <Data>
        <Element type="TextViewElement" name="ProcedureNotes"/>
        <Element type="TextElement" name="Crystalloid"/>
        <Element type="TextElement" name="Ffp"/>
        <Element type="TextElement" name="Albumin"/>
        <Element type="TextElement" name="Prbcs"/>
        <Element type="TextElement" name="Plts"/>
        <Element type="TextElement" name="Hetastarch"/>
        <Element type="TextElement" name="CellSaver"/>
        <Element type="TextElement" name="Cryo"/>
        <Element type="TextElement" name="Ebl"/>
        <Element type="TextElement" name="Urine"/>
    </Data>
    <Graphics>
        <Group orientation="vertical" label="Post-Op Diagnosis">
            <Group orientation="horizontal">
                <Group orientation="vertical" label="IN (mL)">
                    <Group orientation="horizontal">
                        <Group orientation="vertical">
                            <Element label="Crystalloid:" />
                            <Element label="Albumin:" />
                            <Element label="Hetastarch:" />
                        </Group>
                        <Group orientation="vertical">
                            <Element name="Crystalloid" />
                            <Element name="Albumin" />
                            <Element name="Hetastarch" />
                        </Group>
                        <Group orientation="vertical" marginT="14">
                            <Element label="PRBCs:" />
                            <Element label="Cell Saver:" />
                        </Group>
                        <Group orientation="vertical" marginT="14">
                            <Element name="Prbcs" />
                            <Element name="CellSaver" />
                        </Group>
                        <Group orientation="vertical">
                            <Element label="FFP: " />
                            <Element label="PLTs:" />
                            <Element label="Cryo" />
                        </Group>
                        <Group orientation="vertical">
                            <Element name="Ffp" />
                            <Element name="Plts" />
                            <Element name="Cryo" />
                        </Group>
                    </Group>
                </Group>
                <Group orientation="vertical" label="OUT (mL)">
                    <Group orientation="horizontal">
                        <Group orientation="vertical">
                            <Element label="EBL: " />
                            <Element label="Urine: " />
                        </Group>
                        <Group orientation="vertical">
                            <Element name="Ebl" />
                            <Element name="Urine" />
                        </Group>
                    </Group>
                </Group>
            </Group>
        </Group>
    </Graphics>
</Section>